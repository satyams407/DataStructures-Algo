import Foundation

enum BinaryTreeNode<T> {
    case empty
    indirect case node(BinaryTreeNode, T, BinaryTreeNode)
    
    var count: Int {
        switch self {
        case let .node(leftNode, _, rightNode):
              return leftNode.count + 1 + rightNode.count
        case .empty:
            return 0
        }
    }
}
